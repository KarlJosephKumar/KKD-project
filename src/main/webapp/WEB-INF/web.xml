<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Room</servlet-name>
        <servlet-class>servlet.RoomServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Room</servlet-name>
        <url-pattern>/room</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Students</servlet-name>
        <servlet-class>servlet.StudentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Students</servlet-name>
        <url-pattern>/students</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Nav</servlet-name>
        <servlet-class>servlet.NavigationViewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Nav</servlet-name>
        <url-pattern>/nav</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateAccount</servlet-name>
        <servlet-class>servlet.CreateAccountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAccount</servlet-name>
        <url-pattern>/create-account</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>servlet.ChangePasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/change-password</url-pattern>
    </servlet-mapping>

</web-app>